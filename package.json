{
  "name": "emzcli",
  "description": "CLI to do some 8mylez shopware stuff",
  "version": "1.1.1",
  "author": "8mylez @8mylez",
  "bin": {
    "emz": "./bin/run"
  },
  "bugs": "https://github.com/8mylez/emzcli/issues",
  "dependencies": {
