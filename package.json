    "@oclif/config": "^1.12.0",
    "@oclif/plugin-help": "^2.1.4",
    "copy-dir": "^0.4.0",
    "del": "^3.0.0",
    "nodegit": "^0.24.1",
    "zip-local": "^0.3.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "globby": "^8.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
